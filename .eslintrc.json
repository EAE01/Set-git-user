{
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins":[
    "react"
  ],
  "rules":{
    "max-len": 0,
    "no-plusplus": [
      2,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-shadow": 0,
    "no-use-before-define": 0,
    "prefer-destructuring": 0
  }
}
